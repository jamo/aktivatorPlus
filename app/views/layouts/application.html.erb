<!DOCTYPE html>
<html>
    <head>
        <title>Jarmon kyselyohjelma</title><!-- ><title>AktivatorPlus</title>-->
        <%= stylesheet_link_tag    "application", :media => "all" %>
        <%= javascript_include_tag "application" %>
        <%= csrf_meta_tags %>

        <!-- Google analytics toimimaan -->
        <script type="text/javascript">

          var _gaq = _gaq || [];
          _gaq.push(['_setAccount', 'UA-26841266-2']);
          _gaq.push(['_trackPageview']);

          (function() {
          var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
          ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
          var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
          })();

        </script>


    </head>
    <body id="kaikki">
      <% if session[:user_id]%>
        <div class="withNavi">
          <% #debugger %>
          <div class="navi">
            <%= render_navigation(:expand_all => true) %>
          </div>
          <!--<%#= render 'layouts/navi'%>
          <%#= render_navigation(:expand_all => true) %>
            <%#= render_navigation(:expand_all => true) %>-->
          <% else%>
          <div class=withOutNavi>
            <%end%>
            <div class="content">
              <%= yield %>
            </div>
          </div>
          <div class="footer">
            <p class="footer">Time now: <%= Time.now.asctime %> </p>
          </div>
    </body>
</html>
